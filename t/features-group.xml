  <SEGMENT id="id" start="start" stop="stop" type="type" version="X.XX" label="label">
      <FEATURE id="id" label="label">
         <TYPE id="id" category="category" reference="yes|no">type label</TYPE>
         <METHOD id="id"> method label </METHOD>
         <START> start </START>
